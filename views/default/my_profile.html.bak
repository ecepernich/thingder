{{extend 'layout.html'}}
<h1>{{=name}}</h1>

<table class="table">
        {{for row in rows:}}
        <tr>
            <td>{{=row.title}}</td>
            <td>{{=row.created_on}}</td>
            <td>{{=row.interests}}</td>
            <td>{{=row.offers}}</td>
            <td><img width="100px"
     src="{{=URL('download', args=row.image)}}" /></td>
        </tr>
        {{pass}}
    </table>
